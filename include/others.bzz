# Naive pseudo-random generation
function randpercentage(robotid) {
    
    if (robotid%NUM_ROBOTS==0) { return 0.17 }
    if (robotid%NUM_ROBOTS==1) { return 0.50 }
    if (robotid%NUM_ROBOTS==2) { return 0.73 }
    if (robotid%NUM_ROBOTS==3) { return 0.13 }
