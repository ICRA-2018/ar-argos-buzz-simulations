    if (robotid%NUM_ROBOTS==15) { return 0.51 }
    if (robotid%NUM_ROBOTS==16) { return 0.79 }
    if (robotid%NUM_ROBOTS==17) { return 0.62 }
    if (robotid%NUM_ROBOTS==18) { return 0.47 }
    if (robotid%NUM_ROBOTS==19) { return 0.11 }
    
    if (mute==0) {
